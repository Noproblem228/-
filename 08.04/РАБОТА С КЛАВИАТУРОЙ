x = 0
y = 0
z = 0
w = 0
def setup():
    size(600,600)
    background(0,255,0)
    global x,y,z,w
    x = 200
    y = 200
    z = 100
    w = 0
def draw():
    global x,y,z,w
def keyPressed():
    global x,y,z,w
    textSize(30)
    text(key,z,y)
    z = z + 20
    if (key == ENTER):
        y = y + 50
        z = 100
    if(key == TAB):
        z = z+80
