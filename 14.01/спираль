angle = 0
x1 = 0
w = 0
d = 0
def setup():
    size(600,600)
    global x1,w,d
    noStroke()
    x = 1
def draw():
    global angle,x1,w,d
    #ellipse(0,0,50,50)
    translate(300,300)
    angle = angle + 1
    x1 = x1 + 0.5
    fill(random(0,255),random(0,255),random(0,255))
    w=(random(0,150))
    d=(random(0,150))
    
    rotate(radians(angle))
    ellipse(x1,0,w,d)
