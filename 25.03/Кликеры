x = 0
y = 0
w = 0
d = 0
def setup():
    size(600,600)
    global x,y,w,d
    background(3,244,255)
    x = 200
    y = 200
    w = 200
    d = 100
def draw():
    global x,y,w,d
    rect(x,y,w,d)
def mouseClicked():
    if(mouseX > x)and( mouseX < (x + w))and(mouseY >y )and(mouseY < (y + d)):
        background(255,0,0)
