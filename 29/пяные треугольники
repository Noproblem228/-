def draw():
    x = random(0,0)
    y = random(0,0)
    fill((random(0,255),random(0,255),random(0,255)) 
    x = random(0,601)
    y = random(0,601)
    x = random(0,601)
    y = random(0,601)
    x = random(0,601)
    y = random(0,601)
    triangle(x,y,x,y,x,y)
